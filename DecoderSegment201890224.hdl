CHIP DecoderSegment201890224 {
    IN A, B, C, D;
    OUT b;

    PARTS:
    AND(a1 = !A, a2 = !C, a3 = D, out = and1);
    AND(a1 = !A, a2 = B, out = and2);
    AND(a1 = A, a2 = !B, a3 = D, out = and3);
    AND(a1 = A, a2 = C, out = and4);
    OR(a1 = and1, a2 = and2, a3 = and3, a4 = and4, out = b);
}
